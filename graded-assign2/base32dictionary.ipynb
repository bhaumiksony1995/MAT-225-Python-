   "source": [
    "# Use for encoding from integers to letters for base 32 encoding\n",
    "\n",
    "alpha={i:chr(65+i) for i in range(26)}\n",
    "alpha.update({26:'2',27:'3',28:'4',29:'5',30:'6',31:'7'})\n",
    "def text(t):\n",
    "    print(alpha[t])"
   ]
